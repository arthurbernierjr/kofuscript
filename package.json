{
  "name": "mambascript",
  "author": "arthurbernierjr",
  "version": "0.5.1",
  "homepage": "https://github.com/arthurbernierjr/mambascript",
  "bugs": "https://github.com/arthurbernierjr/mambascript/issues",
  "description": "MambaScript The Love Child Of Typescript and Python",
  "keywords": [
    "typedcoffeescript",
    "coffeescript",
    "javascript",
    "language",
    "compiler",
    "mambascript",
    "typescript"
  ],
  "main": "./lib/module",
  "bin": {
    "mamba": "./bin/mamba",
    "mambascript": "./bin/mambascript"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/arthurbernierjr/mambascript.git"
  },
  "scripts": {
    "buildfiles": "make -j build",
    "test": "jest",
    "build": "node build.js",
    "start": "./bin/mamba",
    "mambaview":"./bin/mamba examples/mambaviewExample.mamba"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "babel-loader": "^8.1.0",
    "cluster": "~0.7.7",
    "grunt": "^1.3.0",
    "grunt-contrib-coffee": "^2.1.0",
    "gulp": "^4.0.2",
    "jest": "^26.4.2",
    "kofu-loader": "^1.0.0",
    "mocha": "^8.1.3",
    "pegjs": "git+https://github.com/dmajda/pegjs.git#bea6b1fde74c8aebf802f9bcc3380c65b241e1b7",
    "semver": "^7.3.2",
    "webpack": "^4.44.1"
  },
  "dependencies": {
    "StringScanner": "~0.0.3",
    "arthur-pavlos-string-reversal": "^1.0.0",
    "coffeescript": "^2.5.1",
    "colors": "^0.6.2",
    "consola": "^2.12.1",
    "globby": "^11.0.1",
    "inquirer": "^7.0.1",
    "kofu-optimist": "^0.6.3",
    "lodash": "^4.17.19",
    "nopt": "~2.1.2",
    "pegjs-each-node": "^0.2.1",
    "transform-imports": "^2.1.1"
  },
  "optionalDependencies": {
    "prettyjson": "*",
    "esmangle": "~0.0.8",
    "source-map": "0.1.11",
    "escodegen": "~0.0.24",
    "cscodegen": "git://github.com/michaelficarra/cscodegen.git#73fd7202ac086c26f18c9d56f025b18b3c6f5383"
  },
  "engines": {
    "node": ">=14.8.x"
  },
  "jest": {
    "moduleFileExtensions": [
      "mamba",
      "js"
    ],
    "transformIgnorePatterns": [],
    "transform": {
      ".*": "<rootDir>/tdd/testprocessor.js"
    },
    "testMatch": [
      "<rootDir>/**/*.spec.mamba"
    ]
  },
  "license": "MIT"
}
